{
  "name": "volleyball-liga-manager",
  "version": "1.0.0",
  "description": "Verwaltungssystem fÃ¼r Volleyball Ligen",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:prod:generate": "prisma generate --schema=prisma/schema.prod.prisma",
    "prisma:dev:generate": "prisma generate --schema=prisma/schema.dev.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.dev.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prod.prisma",
    "prisma:prod:reset": "prisma migrate reset --schema=prisma/schema.prod.prisma",
    "prisma:seed:base": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "prisma:seed:assign-teams": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/assign-teams-to-league.ts",
    "prisma:seed:fixtures": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-fixtures.ts",
    "prisma:seed:historical:all": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-historical-leagues.ts",
    "prisma:seed:all": "npm run prisma:seed:base && npm run prisma:seed:assign-teams && npm run prisma:seed:fixtures"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "@prisma/client": "^6.6.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "next": "^14.2.0",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^11.0.5",
    "react-tsparticles": "^2.12.2",
    "slugify": "^1.6.6",
    "tsparticles": "^3.8.1",
    "tsparticles-engine": "^2.12.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.24",
    "prisma": "^6.6.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
