generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Team {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  isAdmin  Boolean @default(false)
}
